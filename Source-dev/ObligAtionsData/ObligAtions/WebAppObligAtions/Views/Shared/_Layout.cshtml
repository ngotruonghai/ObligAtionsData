
<!DOCTYPE html>
<html lang="en">
<head>

    <!-- Required meta tags -->
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <!-- Meta -->
    <meta name="description" content="">
    <meta name="author" content="Themepixels">
    <title>Home</title>
    <!-- Favicon -->
    <!-- <link rel="shortcut icon" type="image/x-icon" href="assets/img/favicon.png"> -->
    <!-- Vendor CSS -->
    <link href="~/css/home/remixicon.css" rel="stylesheet" />
    
    <!-- Template CSS -->
    <link href="~/css/home/style.min.css" rel="stylesheet" />
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/2.1.4/jquery.min.js"></script>
</head>
<body class="skin-base animate">
    <div class="sidebar">
        <div class="sidebar-header">
            <a href="/Home/Index" class="sidebar-logo-text">
                Tân Thuận Đông Q7
            </a>
        </div>
        @*<div class="sidebar-search">
        <div class="search-body">
        <i data-feather="search"></i>
        <input type="text" class="form-control" placeholder="Search...">
        </div>
        </div>*@
        <div class="sidebar-body">
            <ul class="nav-sidebar">
                @Html.Raw(@ViewBag.Greeting)
            </ul>
        </div>
        <div class="sidebar-footer">
            <a href="" class="avatar online"><span class="avatar-initial">US</span></a>
            <div class="avatar-body">
                <div class="d-flex align-items-center justify-content-between">
                    <h5>@ViewBag.UserName</h5>
                </div>
                <a href="/Login/Index" class="footer-menu"><h6>đăng xuất</h6></a>
                @*<span>Superuser/Administrator</span>*@
            </div><!-- avatar-body -->           
        </div>
    </div>

    <div class="content">
        <div class="content-header">
            <a id="contentMenu" href="#" class="content-menu d-none d-lg-flex"><i data-feather="menu"></i></a>
            <a id="mobileMenu" href="#" class="content-menu d-lg-none"><i data-feather="menu"></i></a>
        </div>
        <div class="row">
            <div class="content-body">
                @RenderBody()
                <div id="test"></div>
                <iframe id="iframe" src="" frameborder="0" style="overflow:hidden;overflow-x:hidden;overflow-y:hidden;height:100%;width:100%;position:absolute;top:10%;left:0px;right:0px;bottom:0px" height="100%" width="100%"></iframe>
            </div><!-- content-body -->
        </div>
    </div>
    <script>
        $(document).ready(function () {

        });
        function Loadpage(url) {
            if (url.length > 0) {
                $("#iframe").attr("src", url);
            }

        }
    </script>

    <script src="~/js/home/jquery.min.js"></script>
    <script src="~/js/home/bootstrap.bundle.min.js"></script>
    <script src="~/js/home/feather.min.js"></script>
    <script src="~/js/home/perfect-scrollbar.common.js"></script>
    <script src="~/js/home/perfect-scrollbar.esm.js"></script>
    <script src="~/js/home/perfect-scrollbar.js"></script>
    <script src="~/js/home/perfect-scrollbar.min.js"></script>
    <script src="~/js/home/script.js"></script>
</body>
</html>
